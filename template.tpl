// Insira aqui o código do modelo.

const queryPermission = require('queryPermission');
const injectScript = require('injectScript');
const createQueue = require('createQueue');
const getContainerVersion = require('getContainerVersion');

const url = 'https://plugins.soclminer.com.br/v1/betag/betag.min.js';
const tagVersion = '1.2.0';

const betagPush = createQueue('betag');

if (queryPermission('access_globals', 'readwrite', 'betag') && queryPermission('read_container_data')) {
  const containerVersion = getContainerVersion();
  betagPush(['init', data, {
    gtmId: containerVersion.containerId,
    tagVersion: tagVersion,
    previewMode: containerVersion.previewMode,
  }]);
}

if (queryPermission('inject_script', url)) {
  injectScript(url, data.gtmOnSuccess(), data.gtmOnFailure());
}

// Chame data.gtmOnSuccess depois que a tag for concluída.
